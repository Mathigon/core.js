{
  "name": "@mathigon/core",
  "version": "0.5.1",
  "description": "TypeScript utilities library containing function wrappers, string and array helper functions, event classes and color utilities.",
  "keywords": [
    "utilities",
    "arrays",
    "strings",
    "events",
    "color"
  ],
  "homepage": "https://mathigon.io/",
  "license": "MIT",
  "author": {
    "name": "Philipp Legner",
    "email": "dev@mathigon.org",
    "url": "https://mathigon.org"
  },
  "main": "dist/core.cjs.js",
  "module": "dist/core.esm.js",
  "main:ts": "index.ts",
  "repository": "mathigon/core.js",
  "scripts": {
    "test": "ts-node node_modules/tape/bin/tape test/**/*.ts",
    "prepublish": "npm test && npm run lint && npm run build",
    "build": "rollup --config",
    "lint": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "5.0.2",
    "@types/tape": "4.13.0",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "eslint": "7.6.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.22.0",
    "rollup": "2.23.1",
    "tape": "5.0.1",
    "ts-node": "8.10.2",
    "tslib": "2.0.1",
    "typescript": "3.9.7"
  },
  "renovate": {
    "extends": [
      "config:base"
    ]
  }
}
